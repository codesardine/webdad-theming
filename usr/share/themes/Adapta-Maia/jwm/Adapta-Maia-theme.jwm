            <Background>#009688</Background>
        </Active>
    </MenuStyle>

    <!-- Visual Style Tray. -->
    <TrayStyle decorations="flat">
        
        <Font>Roboto-12:weight=bold:antialias=true:encoding=utf8</Font>
